pipeline {
	agent any
		stages {
		stage('build') {
			steps {
				echo 'building the application...'
			}
		}
		stage('test') {
			steps {
				echo 'testing the application...'
			}
		}
		stage('deploy') {
			steps {
				echo "deplying the application..."
			}
		}
		stage('Four') {       
			steps {
			echo "Running another test job"
			}        
		}
	}
}
